{
    "parser": "babel-eslint",
    "extends": [
        "plugin:flowtype/recommended"
    ],
    "plugins": [
        "flowtype"
    ],

    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2
    }
}
