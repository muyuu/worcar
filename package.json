{
  "name": "worcar",
  "version": "0.0.1",
  "description": "Word Dictionaly Tool for language beginners.",
  "repository": "https://github.com/muyuu/worcar/",
  "main": "index.js",
  "scripts": {
    "build:js": "webpack",
    "watch:js": "webpack --progress --colors --watch",
    "build:css": "node-sass --output-style compressed src/css/style.sass dist/style.css",
    "watch:css": "node-sass -w src/css/style.sass dist/style.css",
    "cp:font": "cpx \"./node_modules/font-awesome/fonts/**/{*.otf,*.eot,*.svg,*.ttf,*.woff*}\" ./dist/fonts",
    "check": "flow check",
    "lint": "eslint ./src",
    "serve": "webpack-dev-server --history-api-fallback",
    "deploy": "firebase deploy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run serve & npm run watch",
    "watch": "npm run watch:css & npm run watch:js"
  },
  "keywords": [],
  "author": "muyuu",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-plugin-flowtype": "^2.29.2",
    "eslint-plugin-react": "^6.8.0",
    "espowerify": "^1.1.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "firebase": "^3.6.4",
    "firebase-tools": "^3.2.1",
    "flow-bin": "^0.37.4",
    "font-awesome": "^4.7.0",
    "node-sass": "^4.1.1",
    "power-assert": "^1.4.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-markdown": "^2.4.4",
    "react-router": "^3.0.1",
    "sass-loader": "^5.0.1",
    "shortid": "^2.2.6",
    "standby.css": "^1.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^1.16.3"
  }
}
